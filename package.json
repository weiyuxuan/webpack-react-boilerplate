{
  "name": "webpack-react-boilerplate",
  "version": "0.0.1",
  "description": "A simple and easy boilerplate for react developer, based on webpack 5.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weiyuxuan/webpack-react-boilerplate.git"
  },
  "author": "weiyuxuan",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config scripts/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config scripts/webpack.prod.js",
    "lint": "eslint -c .eslintrc --ext .ts,.tsx,.js,.jsx ./src --fix",
    "stylelint": "stylelint 'src/**/*.scss' --customSyntax=postcss-scss --fix",
    "prepare": "husky install"
  },
  "bugs": {
    "url": "https://github.com/weiyuxuan/webpack-react-boilerplate/issues"
  },
